{
  "manifest_version": 3,
  "name": "Dex et Calc Run and Bun FR",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
  {
  "matches": ["https://dex.runandbun.com/*"],
  "js": ["translation.js","dex.js"],
  "run_at": "document_idle"
  },{
  "matches": ["https://calc.runandbun.com/"],
  "js": ["translation.js","calc.js"],
  "run_at": "document_idle"
  }]
}